<!DOCTYPE html>
<html>
<head>
	<title>Service Form</title>
	<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
	<script>
		fetch("http://localhost:3000/api/v1/service/list/Instagram", {
            method : "get" ,
        })
        .then(response => response.json())
        .then(data => data.forEach(e => {
            console.log(e.name);
            document.getElementById("services-dropdown").innerHTML += 
            `<option>${e.name}</option>`
        }))
        .catch(error => console.error(error));
	</script>
</head>
<body>
	<form>
		<label for="category">Category:</label>
		<input type="text" id="category" name="category"><br><br>

		<label for="services-dropdown">Service:</label>
		<select id="services-dropdown" name="service"></select><br><br>

		<label for="link">Link:</label>
		<input type="text" id="link" name="link"><br><br>

		<label for="quantity">Quantity:</label>
		<input type="number" id="quantity" name="quantity"><br><br>

		<label for="average-time">Average Time:</label>
		<input type="text" id="average-time" name="average-time"><br><br>

		<label for="charge">Charge:</label>
		<input type="number" id="charge" name="charge"><br><br>

		<input type="submit" value="Submit">
	</form>
</body>
</html>
